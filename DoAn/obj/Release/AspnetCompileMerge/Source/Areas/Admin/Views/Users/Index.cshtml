@model List<DoAn.Identity.AppUser>

@{
    ViewBag.Title = "Users";
    Layout = "~/Areas/Admin/Views/Shared/_AdminLayoutPage.cshtml";
}

<h2>List Of Users</h2>

<table class="table table-bordered">
    <tr>
        <th>Username</th>
        <th>Email</th>
        <th>Mobile</th>
        <th>Date Of Birth</th>
        <th>City</th>

    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.UserName</td>
            <td>@item.Email</td>
            <td>@item.PhoneNumber</td>
            <td>@item.Birthday</td>
            <td>@item.Address</td>
        </tr>
    }
</table>